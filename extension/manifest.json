{
    "manifest_version": 3,
    "name": "SafeBrowse",
    "version": "0.1.0",
    "permissions": [
      "tabs",
      "notifications",
      "storage"
    ],
    "host_permissions": [
      "<all_urls>",
      "https://safebrowsing.googleapis.com/*"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
      }
    },
    "icons": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  }
  